{
    "problemMatcher": [
      {
        "owner": "hlint",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "^(.+):(\\d+):(\\d+)(-\\d+)?: (Error|Warning|Suggestion): (.+)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 6
          }
        ]
      }
    ]
  }
